  "tyxml" {= "4.6.0" & with-doc}
  "uutf" {= "1.0.3" & with-doc}
  "zarith" {= "1.14" & with-test}
]
build: [
  "dune"
  "build"
  "-p"
  name
  "-j"
  jobs
  "@runtest" {with-test}
  "@doc" {with-doc}
]
dev-repo: "git+https://github.com/eilvelia/ocaml-kdl.git"
tags: ["data-serialization-format" "configuration-format"]
