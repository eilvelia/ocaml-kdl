  "conf-gmp" {= "4" & with-test}
  "conf-pkg-config" {= "3" & with-test}
  "cppo" {= "1.8.0" & with-doc}
  "crunch" {= "3.3.1" & with-doc}
  "csexp" {= "1.5.2" & with-test}
