bug-reports: "https://github.com/eilvelia/ocaml-kdl/issues"
depends: [
  "astring" {= "0.8.5" & with-doc}
  "base" {= "v0.17.1" & with-test}
  "base-bigarray" {= "base"}
  "base-domains" {= "base"}
  "base-nnp" {= "base"}
  "base-threads" {= "base"}
